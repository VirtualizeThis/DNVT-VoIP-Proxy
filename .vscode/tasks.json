{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Main DNVT Proxy Executable",
            "type": "shell",
            "command": "gcc",
            "args": [
                "main.c",
                "register_phone.c",
                "-lrt",
                "-o",
                "main",
                "-Incurses",
                "-lcurses",
                "-lpthread",
                "-ltinfo",
                "-lpj",
                "-lpjsip",
                "-lpjsip-ua",
                "-lpjsip-simple",
                "-lpjsua",
                "-lpjmedia",
                "-lpjmedia-codec",
                "-lpjmedia-audiodev",
                "-lpjnath",
                "-lpjlib-util",
                "-I/usr/include/libusb-1.0",
                "-lusb-1.0"
            ]
        },
        {
            "label": "Phone Registration Test Utility",
            "type": "shell",
            "command": "gcc",
            "args": [
                "register_phone.c",
                "phone_reg_utility.c",
                "-lrt",
                "-o",
                "phone_reg_utility",
                "-Incurses",
                "-lcurses",
                "-lpthread",
                "-ltinfo",
                "-lpj",
                "-lpjsip",
                "-lpjsip-ua",
                "-lpjsip-simple",
                "-lpjsua",
                "-lpjmedia",
                "-lpjmedia-codec",
                "-lpjmedia-audiodev",
                "-lpjnath",
                "-lpjlib-util",
                "-I/usr/include/libusb-1.0",
                "-lusb-1.0"
            ]
        }
    ],
    "group": {
        "kind": "build",
        "isDefault": true
    },
    "problemMatcher": "$gcc"
}